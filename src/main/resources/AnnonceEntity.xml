<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="fr.michka.lebonbien.model.AnnonceEntity">
        <table name="ANNONCE" schema="michkaDB" catalog=""/>
        <attributes>
            <id name="idAnnonce">
                <column name="ID_ANNONCE" precision="-1"/>
            </id>
            <basic name="dateAnnonce">
                <column name="DATE_ANNONCE" precision="-1"/>
            </basic>
            <basic name="idBien">
                <column name="ID_BIEN" precision="-1"/>
            </basic>
            <basic name="idAgent">
                <column name="ID_AGENT" precision="-1"/>
            </basic>
            <basic name="idProprietaire">
                <column name="ID_PROPRIETAIRE" precision="-1"/>
            </basic>
            <basic name="titre">
                <column name="TITRE"/>
            </basic>
            <basic name="description">
                <column name="DESCRIPTION"/>
            </basic>
            <basic name="prix">
                <column name="PRIX" precision="2"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
