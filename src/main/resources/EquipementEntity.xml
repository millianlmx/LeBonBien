<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="fr.michka.lebonbien.model.EquipementEntity">
        <table name="EQUIPEMENT" schema="michkaDB" catalog=""/>
        <attributes>
            <id name="idEquipement">
                <column name="ID_EQUIPEMENT" precision="-1"/>
            </id>
            <basic name="libEquipement">
                <column name="LIB_EQUIPEMENT" length="50"/>
            </basic>
            <basic name="valeur">
                <column name="VALEUR" length="50"/>
            </basic>
            <basic name="idBien">
                <column name="ID_BIEN" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
